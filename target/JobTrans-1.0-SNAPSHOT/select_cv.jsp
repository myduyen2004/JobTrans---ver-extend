<style>.cv-container {
    width: 80%;
    max-width: 750px;
    background: white;
    padding: 20px;
    border-radius: 10px;
}

.cv-title {
    color: #6c63ff;
    font-size: 20px;
    margin-bottom: 40px;
    border: 2px solid #6c63ff;
    display: inline-block;
    padding: 8px 15px;
    border-radius: 20px;
    margin-top: 20px;
}

.cv-card {
    display: flex;
    align-items: center;
    border: 2px solid #6c63ff;
    border-radius: 10px;
    padding: 15px;
    margin-bottom: 15px;
    width:1000px;
    height: 200px;
    
}

.cv-image {
    width: 130px;
    height: 170px;
    object-fit: cover;
    border-radius: 20px;
}

.cv-info {
    flex: 1;
    margin-left: 15px;
}

.cv-description {
    margin-left: 20px;
    font-size: 16px;
    margin-bottom: 10px;
}

.cv-price {
    display: inline-block;
    background: #f8e78f;
    color: black;
    font-size: 14px;
    padding: 4px 10px;
    border-radius: 12px;
    font-weight: bold;
}

.cv-price.free {
    background: #f8e78f;
    color: black;
}

.button-group {
    display: flex;
    margin-left: 60px;
    gap: 40px; /* Khoảng cách giữa các nút */
    margin-top: 15px;
    
}

.view-btn, .use-btn {
    margin-top: 20px;
    width: 200px;
    height: 45px;
    padding: 8px 16px;
    border-radius: 20px;
    font-size: 14px;
    border: none;
    cursor: pointer;
    transition: all 0.3s;
}

.view-btn {
    background: #d9d9ff;
    color: black;
}

.use-btn {
    background: #b0f5c8;
    color: black;
}

.view-btn:hover {
    background: #6c63ff;
    color: white;
}

.use-btn:hover {
    background: #37d67a;
    color: white;
}
.cv-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 10px;
}

.cv-description {
    font-size: 16px;
    font-weight: bold;
    color: #333;
    flex: 1;
}

.cv-price {
    background: #f8e78f;
    color: black;
    font-size: 14px;
    padding: 4px 12px;
    border-radius: 12px;
    font-weight: bold;
    white-space: nowrap; /* Giữ nội dung không bị xuống dòng */
}
</style>

